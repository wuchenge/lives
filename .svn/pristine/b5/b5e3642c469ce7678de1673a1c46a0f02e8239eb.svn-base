<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>我的爱好</title>
	<meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
	<link rel="stylesheet" type="text/css" href="../../../res/mui/css/mui.css"/>
	<link rel="stylesheet" type="text/css" href="../../../res/mui/css/mui_min.css"/>
</head>
<style>
	.header_right{
		position: absolute;
		position: absolute;
		text-align: center;
		height: 44px;
		line-height: 44px;
		right: 0px;
		width: 50px;
	}
</style>
<body>
		<header class="mui-bar mui-bar-nav" style="height:62px">
			<a tapmode onclick="closeaihao_win()" class="mui-icon mui-icon-left-nav mui-pull-left"></a>
			<h1 class="mui-title">爱好</h1>
			<div class="header_right">保存</div>
		</header>
</body>
<script type="text/javascript" src="../../../res/mui/js/mui_min.js"></script>
<script type="text/javascript" src="../../../res/mui/js/mui.js"></script>
<script type="text/javascript" src="../../../script/api.js"></script>
<script type="text/javascript" src="../../../script/js.js"></script>
<script type="text/javascript">
apiready = function () {
		$api.fixStatusBar( $api.dom('header'));
		openaihao_frm();
}

function openaihao_frm(){
	api.openFrame({
	    name: 'aihao_frm',
	    url: 'aihao_frm.html',
	    rect: {
	        x: 0,
	        y: $api.offset($api.dom('header')).h,
	        w: 'auto',
	        h: 'auto'
	    },
	    pageParam: {
	        name: 'test'
	    },
	    bgColor: 'rgba(0,0,0,0)',
	    vScrollBarEnabled: true,
	    hScrollBarEnabled: true
	});
}
function closeaihao_win(){
	api.closeWin({
	    name: 'aihao_win'
	});

}
</script>
</html>
